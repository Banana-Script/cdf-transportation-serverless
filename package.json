    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.5",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@auth/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@notifications/notifications(|/.*)$": "<rootDir>/libs/notifications/src/$1",
      "^OpenAI/open-ai(|/.*)$": "<rootDir>/libs/open-ai/src/$1",
      "^@app/whatsapp(|/.*)$": "<rootDir>/libs/whatsapp/src/$1",
      "^vapi/vapi(|/.*)$": "<rootDir>/libs/vapi/src/$1",
      "^@app/twilio(|/.*)$": "<rootDir>/libs/twilio/src/$1",
      "^@app/google(|/.*)$": "<rootDir>/libs/google/src/$1",
      "^miner/dialog-miner(|/.*)$": "<rootDir>/libs/dialog-miner/src/$1"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
