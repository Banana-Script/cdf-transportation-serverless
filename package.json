  "engines": {
    "node": ">=18 <= 22"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "bash ./build.sh",
    "build:multi": "bash ./build_multi.sh",
    "build:custom": "nest build users  --webpack",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "npm run serverless:start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/viernes-business-services/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/organizations/test/jest-e2e.json",
    "serverless:start": "npm run build  && npx serverless offline --host 0.0.0.0",
    "serverless:run": "npx serverless offline --host 0.0.0.0 --httpPort 3500",
    "serverless:run:dev": "npx serverless offline --host 0.0.0.0 --httpPort 3500 --aws-profile viernes-business-services-dev",
    "serverless:runts": "npx serverless offline --config serverless.local.yml  --host 0.0.0.0 --httpPort 3500",
    "serverless:start:custom": "npm run build:custom  && npx serverless offline --host 0.0.0.0",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:create": "npm run typeorm -- migration:create -n",
    "migrations:run": "npm run typeorm -- migration:run -f ormconfig.ts && npm run seed:run",
    "migrate": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:revert": "npm run typeorm -- migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "pretty": "prettier --write \"./**/*.{ts,js,jsx,tsx,json}\"",
    "prepare": "husky install"
  },
